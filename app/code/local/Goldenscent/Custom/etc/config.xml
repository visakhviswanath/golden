<?xml version="1.0"?>
<config>
    <modules>
      <Goldenscent_Custom>
        <version>0.1.0</version>
      </Goldenscent_Custom>
    </modules>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <Goldenscent_Custom before="Mage_Adminhtml">Goldenscent_Custom_Adminhtml</Goldenscent_Custom>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
    <global>
        <blocks>
            <adminhtml>
              <rewrite>
                  <sales_order_grid>Goldenscent_Custom_Block_Adminhtml_Sales_Order_Grid</sales_order_grid>
              </rewrite>
            </adminhtml>
        </blocks>
        <helpers>
            <custom>
              <class>Goldenscent_Custom_Helper</class>
            </custom>
        </helpers>
        <models>
            <custom>
                <class>Goldenscent_Custom_Model</class>
                <resourceModel>custom_mysql4</resourceModel>
            </custom>
        </models>
        <resources>
            <custom_setup>
                <setup>
                    <module>Goldenscent_Custom</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </custom_setup>
          <custom_write>
                <connection>
                    <use>core_write</use>
                </connection>
          </custom_write>
          <custom_read>
                <connection>
                    <use>core_read</use>
                </connection>
          </custom_read>
          <salesattribute1551979825_setup>
                <setup>
                    <module>Goldenscent_Custom</module>
                    <class>Mage_Sales_Model_Mysql4_Setup</class>
                </setup>
                <connection>
                  <use>core_setup</use>
                </connection>
          </salesattribute1551979825_setup>
          <salesattribute1551979825_write>
                <connection>
                  <use>core_write</use>
                </connection>
          </salesattribute1551979825_write>
          <salesattribute1551979825_read>
                <connection>
                  <use>core_read</use>
                </connection>
          </salesattribute1551979825_read>
        </resources>
        <events>
            <sales_order_place_after> <!-- identifier of the event we want to catch -->
                <observers>
                    <sales_order_place_after_handler> <!-- identifier of the event handler -->
                        <type>model</type> <!-- class method call type; valid are model, object and singleton -->
                        <class>custom/observer</class> <!-- observers class alias -->
                        <method>savePartner</method>  <!-- observer's method to be called -->
                        <args></args> <!-- additional arguments passed to observer -->
                    </sales_order_place_after_handler>
                </observers>
          </sales_order_place_after>     
        </events>
    </global>
</config> 